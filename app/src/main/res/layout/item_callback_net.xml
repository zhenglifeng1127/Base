<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:gravity="center"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:background="@color/white"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <TextView
        android:id="@+id/_itemEmpty"
        android:drawablePadding="@dimen/dp_24"
        android:layout_width="@dimen/dp_165"
        android:layout_height="wrap_content"
        android:textSize="@dimen/sp_13"
        android:layout_gravity="center"
        android:textColor="@color/tc3"
        android:gravity="center"
        android:text="@string/netError" />

    <com.nxhz.base.ui.CornerText
        android:layout_marginTop="@dimen/dp_32"
        android:gravity="center"
        android:textColor="@color/white"
        android:text="@string/loadNew"
        app:cornerTextBgColor="@color/mainColor"
        app:cornerTextRadius="@dimen/dp_13"
        android:layout_width="@dimen/dp_145"
        android:layout_height="@dimen/dp_32"/>
</LinearLayout>
